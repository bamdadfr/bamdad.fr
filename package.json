    "@react-spring/web": "^9.3.2",
    "@sentry/nextjs": "^6.14.2",
    "next": "^12.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-use-measure": "^2.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^2.0.2",
    "@bamdadsabbagh/stylelint-config": "^1.3.19",
