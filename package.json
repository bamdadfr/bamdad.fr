    "@react-spring/web": "^9.6.1",
    "@sentry/nextjs": "^7.29.0",
    "just-shuffle": "^4.2.0",
    "next": "^13.1.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.0"
