    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-jest": "^29.3.1",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
