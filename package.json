  "scripts": {
    "build": "pnpm build:run && yarn build:post",
    "build:run": "next build",
    "build:post": "find .next/static -type f -name \"*.map\" -exec rm {} \\;",
    "coverage": "pnpm test:coverage",
    "clean": "pnpm next:clean && yarn package:clean",
    "dev": "next dev",
    "export": "next export",
    "lint": "eslint src --ext .js --fix",
    "next:clean": "rimraf .next",
    "package:clean": "rimraf node_modules",
    "package:reinstall": "pnpm package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
