    "@react-spring/web": "^9.7.3",
    "@sentry/nextjs": "^7.80.0",
    "just-shuffle": "^4.2.0",
    "next": "^14.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.1"
  },
