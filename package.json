    "@types/styled-components": "^5.1.12",
    "babel-jest": "^27.0.2",
    "babel-plugin-styled-components": "^2.0.1",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.0.3",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "jest": "^27.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.43.5",
    "sass-reset": "^1.0.3"
  }
