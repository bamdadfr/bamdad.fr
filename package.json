    "scripts": {
        "build": "next build && yarn build:post",
        "build:post": "find .next -type f -name \"*.map\" -exec rm {} \\;",
        "clean": "yarn next:clean && yarn package:clean",
        "dev": "next dev",
        "export": "next export",
        "lint": "eslint src --ext .js --fix",
        "next:clean": "rimraf .next",
        "package:clean": "rimraf node_modules",
        "package:reinstall": "yarn package:clean && yarn",
        "package:sort": "npx sort-package-json",
        "start": "next start",
        "test": "jest",
        "test:cov": "yarn test:coverage",
        "test:coverage": "jest --coverage && codecov",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@fontsource/lora": "^4.5.0",
        "@iconify/icons-simple-icons": "^1.1.23",
        "@iconify/react": "^3.0.0",
        "@juggle/resize-observer": "^3.3.1",
        "@react-spring/web": "^9.2.4",
        "@sentry/nextjs": "^6.12.0",
        "next": "^11.1.2",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-use-measure": "^2.0.4",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@bamdadsabbagh/eslint-config": "^1.16.0",
        "@bamdadsabbagh/stylelint-config": "^1.3.19",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/react": "^17.0.18",
        "@types/react-dom": "^17.0.9",
        "@types/styled-components": "^5.1.12",
        "babel-jest": "^27.0.2",
        "babel-plugin-styled-components": "^1.13.2",
        "codecov": "^3.8.3",
        "cross-env": "^7.0.3",
        "eslint-config-next": "^11.1.2",
        "eslint-plugin-jest-dom": "^3.9.0",
        "eslint-plugin-testing-library": "^4.10.1",
        "jest": "^27.0.4",
        "rimraf": "^3.0.2",
        "sass": "^1.39.0",
        "sass-reset": "^1.0.3"
    }
